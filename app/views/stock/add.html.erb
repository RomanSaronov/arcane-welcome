<!DOCTYPE html>
<html>
  <head>
  </head>
  <body>
    <div class="container">
      <div class="row">
        <div class="col-12">
          <h1>Add Stock</h1>
        </div>
      </div>
      <div class="row bg-light p-3">
          <div class="col-6">

            <%= form_for @posts, url: {action: "create"} do |f| %>
              <%= f.text_field :name %>
              <%= f.text_field :price %>
              <%= f.text_field :duration %>
              <%= f.text_field :interest %>

              <%= f.submit "Create" %>
            <% end %>


          <div class="col-6"></div>
        </div>
        </div>
        <div class="col-6"></div>
      </div>
    </div>
  </body>
</html>


